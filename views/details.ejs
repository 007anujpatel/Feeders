<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('../partials/head') %>
</head>
<body class="container">

<header>
    <%- include('../partials/header') %>
</header>

<main>
    <div class="jumbotron" style="padding:40px;">
        <p><%= message || '' %></p>
        <ul>
            <% for (var prop in data) { %>
            <li><%=prop.toUpperCase()%> - <%=data[prop]%></li>
            <% } %>
        </ul>
    </div>
</main>


<footer>
    <%- include('../partials/footer') %>
</footer>

</body>
</html>